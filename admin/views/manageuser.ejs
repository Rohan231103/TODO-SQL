<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Manage User</title>
</head>
<body>
    <center>
    <h3>* Manage User!</h3>
    <table border="1">
        
        <th>User Id</th>
        <th>User Name</th>
        <th>User Email</th>
        <th>User Password</th>
        <th>Update User</th>
        <th>Delete User</th>

    
        <% results.forEach(function(user){%>
            <tr>
                <td><%= user.id%></td>
                <td><%= user.name%></td>
                <td><%= user.email%></td>
                <td><%= user.password%></td>
                <td><a href="update/<%= user.id %>">Update</a></td>
                <td><a href="delete/<%= user.id %>">delete</a></td>
            </tr>
        <%})%>
    </table><br> 
</center>
</body>
</html>